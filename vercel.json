{
  "redirects": [
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "consoledotlog.in"
        }
      ]
    },
    {
      "source": "/:path*",
      "destination": "https://consoledotlog.in/:path*",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.consoledotlog.in"
        }
      ]
    }
  ]
}
